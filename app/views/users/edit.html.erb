<h1>Edit your profile</h1>
<%= form_for @user do |f| %>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.email_field :email, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :nick_name %>
    <%= f.text_field :nick_name, class: "form-control" %>
  </div>

  <%= f.submit class: "btn btn-primary" %>
<% end %>
