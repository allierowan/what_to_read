<div class="container-fluid">
  <h1>Browse the Library</h1>
  <ul>
    <% @books.each do |book| %>
      <li class="list-unstyled">
        <h3><%= link_to book.title, book_path(book.id)%></h3>
        <p>
          by <%= book.author.full_name %>
        </p>
        <p>
          <%= book.genre %>
        </p>
      </li>
    <% end %>
  </ul>
</div>
